//
// All profiles begin with this data and overwrite their own
//
Default
	Skill = 49
	Aggression = 50
	ReactionTime = 0.33
	AttackDelay = 0
	Teamwork = 75
	AimFocusInitial = 6				// initial focus spread in degrees (from desired center)
	AimFocusDecay = 0.25			// how much focus shrinks per second (.25 = 25% of size after 1 sec)
	AimFocusOffsetScale = 0.525		// controls accuracy when tracking to target (0 == perfect, should always be < 1)
	AimfocusInterval = 0.9			// how often focus is adjusted (smaller intervals means better movement tracking)
	Difficulty = NORMAL
	LookAngleMaxAccelNormal = 25000
	LookAngleStiffnessNormal = 1800.0
	LookAngleDampingNormal = 50.0
	LookAngleMaxAccelAttacking = 45000
	LookAngleStiffnessAttacking = 2200.0
	LookAngleDampingAttacking = 36.1
End

